n = int(input())
graph=[[] for _ in range(n+1)]

for i in range(n-1):
    a,b = map(int, input().split())
    graph[a].append(b)
    graph[b].append(a)

parent = [0]*n+1

def dfs(node):
    for nextnode in graph[node]:
        if parent[nextnode]==0:
            parent[nextnode]=node
            dfs(nextnode)

dfs(1)

for i in range(2,n+1):
    parent[i]